   "source": [
    "def power(x, n):\n",
    "    res = 1\n",
    "     \n",
    "    while (n > 0):\n",
    "        if ((n & 1) == 1) :\n",
    "            res = res * x\n",
    "        n = n // 2\n",
    "        x = x * x\n",
    "     \n",
    "    return res\n",
    "x = 10\n",
    "n = 2\n",
    " \n",
    "print(\"Power is \", power(x, n))"
   ]
  }
